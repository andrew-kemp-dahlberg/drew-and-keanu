# Copy this to terraform.tfvars and fill in your values

# Your S3 bucket name (must be globally unique)
# If you have a domain, use that: "clawdbot.com"
# Otherwise use something unique: "clawdbot-site-andrew"
domain_name = "your-bucket-name-here"

# AWS region
aws_region = "us-east-1"

# Set to true if you want HTTPS via CloudFront
enable_cloudfront = false
